{
  "name": "react-boilerplate",
  "version": "1.0.0",
  "main": "src/index.ts",
  "author": "kobakakzu0429 <kobakazu10@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build:client": "webpack --color --progress --config ./config/webpack.config.prod.js",
    "dev:client": "webpack-dev-server --config ./config/webpack.config.dev.js",
    "build:server": "tsc --project .",
    "dev:server": "yarn build:server --watch",
    "start:ssr": "wait-on dist/server/index.js && node dist/server/index.js",
    "lint": "tslint --fix -c ./tslint.json '{client,common,server}/**/*{.ts,.tsx}'",
    "clean": "rm -rf dist"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "yarn lint",
      "git add"
    ]
  },
  "dependencies": {
    "express": "^4.17.1",
    "normalize.css": "^8.0.1",
    "react": "^16.9.0",
    "react-dom": "^16.12.0",
    "react-router-dom": "^5.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.2",
    "@types/node": "^20.0.0",
    "@types/react": "16.9.17",
    "@types/react-dom": "^16.9.4",
    "@types/react-router-dom": "5.1.3",
    "clean-webpack-plugin": "4.0.0",
    "css-loader": "7.1.2",
    "file-loader": "6.2.0",
    "fork-ts-checker-webpack-plugin": "9.0.2",
    "html-webpack-plugin": "5.6.0",
    "npm-run-all": "^4.1.5",
    "prettier": "3.3.3",
    "style-loader": "4.0.0",
    "ts-loader": "9.5.1",
    "tslint": "6.1.3",
    "tslint-config-prettier": "1.18.0",
    "tslint-loader": "3.5.4",
    "tslint-plugin-prettier": "2.1.0",
    "typescript": "5.5.4",
    "url-loader": "4.1.1",
    "wait-on": "^8.0.0",
    "webpack": "5.94.0",
    "webpack-cli": "5.1.4",
    "webpack-dev-server": "5.0.4",
    "webpack-manifest-plugin": "5.0.0",
    "webpack-merge": "6.0.1"
  }
}
