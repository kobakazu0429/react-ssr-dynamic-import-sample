{
  "name": "react-boilerplate",
  "version": "1.0.0",
  "main": "src/index.ts",
  "author": "kobakakzu0429 <kobakazu10@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build:client": "webpack --color --progress --config ./config/webpack.config.prod.js",
    "dev:client": "webpack-dev-server --config ./config/webpack.config.dev.js",
    "build:server": "tsc --project .",
    "dev:server": "yarn build:server --watch",
    "start:ssr": "wait-on dist/server/index.js && node dist/server/index.js",
    "lint": "tslint --fix -c ./tslint.json '{client,common,server}/**/*{.ts,.tsx}'",
    "clean": "rm -rf dist"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "yarn lint",
      "git add"
    ]
  },
  "dependencies": {
    "express": "^4.17.1",
    "normalize.css": "^8.0.1",
    "react": "^16.9.0",
    "react-dom": "^16.12.0",
    "react-router-dom": "^5.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.2",
    "@types/node": "^13.1.6",
    "@types/react": "16.9.17",
    "@types/react-dom": "^16.9.4",
    "@types/react-router-dom": "5.1.3",
    "clean-webpack-plugin": "3.0.0",
    "css-loader": "3.4.2",
    "file-loader": "5.0.2",
    "fork-ts-checker-webpack-plugin": "3.1.1",
    "html-webpack-plugin": "3.2.0",
    "npm-run-all": "^4.1.5",
    "prettier": "1.19.1",
    "style-loader": "1.1.2",
    "ts-loader": "6.2.1",
    "tslint": "5.20.1",
    "tslint-config-prettier": "1.18.0",
    "tslint-loader": "3.5.4",
    "tslint-plugin-prettier": "2.1.0",
    "typescript": "3.7.4",
    "url-loader": "3.0.0",
    "wait-on": "^3.3.0",
    "webpack": "5.94.0",
    "webpack-cli": "3.3.10",
    "webpack-dev-server": "3.10.1",
    "webpack-manifest-plugin": "2.2.0",
    "webpack-merge": "4.2.2"
  }
}
